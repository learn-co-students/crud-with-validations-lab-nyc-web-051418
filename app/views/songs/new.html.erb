<h1>New Song</h1>
<%= form_for @song do |f| %>
<label>Song Title:</label>
<div><% f.label :title %>
    <%= f.text_field :title %>
</div><br>

<label>Artist Name:</label>
<div><% f.label :artist_name %>
    <%= f.text_field :artist_name %>
</div><br>

<label>Genre:</label>
<div><% f.label :genre %>
    <%= f.text_field :genre %>
</div><br>

<label>Song is Released?</label><%= f.check_box :released %><br><br>

<label>Year of Song's Release:</label>
<div><% f.label :release_year %>
    <%= f.text_field :release_year %>
</div><br>
<%= f.submit %>
<% end %>
