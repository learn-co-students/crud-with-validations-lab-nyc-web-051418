<h1>
  <%= @song.title %>
</h1>

<h2>
  Artist:
  <%= @song.artist_name %>
</h2>
<h2>
  Released?
  <%= @song.released ? "Yes" : "No" %>
</h2>
<h2>
  Release year:
  <%= @song.release_year %>
</h2>
<h2>
  Genre:
  <%= @song.genre %>
</h2>

<p><%= link_to "Edit", edit_song_path(@song) %></p>
<%= button_to 'Delete', song, :method => :delete %>
